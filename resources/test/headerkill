proxy_set_header x-date $msec;
proxy_set_header x-cyn-date $timestamp;
proxy_set_header x-rev-date $msec;
proxy_set_header x-debug-cyn-headers off;
proxy_set_header x-debug-cyn-ip on;
proxy_set_header x-debug-cyn-version on;
proxy_set_header x-debug-cyn-profile on;
proxy_set_header x-debug-cyn-min-version -.-;
proxy_set_header X-Real-IP $remote_addr;
proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
proxy_set_header x-cyn-secure $scheme;
proxy_set_header $kill;

